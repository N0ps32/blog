{
  "require": {
    "craftcms/cms": "3.3.15",
    "craftcms/redactor": "2.4.0",
    "luwes/craft3-codemirror": "1.0.1",
    "raeder/craft-http2-server-push-twig": "^1.0",
    "supercool/tablemaker": "2.0.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
